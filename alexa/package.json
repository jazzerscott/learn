{
  "name": "learn-alexa",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "concurrently -k -p name -n \"tsc,svr\" -c \"blue,green\" \"npm run build:watch\" \"npm run serve\"",
    "build": "rm -rf dist && tsc",
    "build:watch": "tsc -w",
    "db-init": "AWS_PROFILE=jazzer node ./dist/db-init.js" ,
    "serve": "AWS_PROFILE=jazzer nodemon --inspect=8201 ./dist/index.js ",
    "lint": "tslint 'src/**/*.*' -e src/**/*.json --force -c ../tslint.json",
    "pretest": "rm -rf test-output && tsc -p test",
    "test": "./node_modules/.bin/istanbul cover _mocha ./test-output/test --no-exit"
  },
  "author": "Scott Williams",
  "license": "ISC",
  "devDependencies": {
    "@types/expect": "1.20.1",
    "@types/express": "4.0.36",
    "@types/mocha": "2.2.41",
    "@types/node": "6.0.63",
    "chai": "4.1.1",
    "concurrently": "3.5.0",
    "istanbul": "0.4.5",
    "mocha": "3.5.0",
    "nodemon": "1.11.0",
    "tslint": "4.5.1",
    "typescript": "2.4.2"
  },
  "dependencies": {
    "aws-sdk": "^2.141.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "inversify": "^4.3.0",
    "inversify-express-utils": "^4.0.1",
    "reflect-metadata": "0.1.9",
    "uuid": "^3.1.0"
  }
}
